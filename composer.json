{
  "name": "roomify/roomify-extension",
  "type": "roomify-extension",
  "description": "Roomify extension for Behat",
  "keywords": ["drupal", "web", "test"],
  "homepage": "https://github.com/Roomify/roomifyextension",
  "license": "GPL-2.0+",
  "require": {
    "behat/mink": "~1.5",
    "behat/mink-goutte-driver": "~1.0",
    "behat/mink-selenium2-driver": "~1.1",
    "behat/behat": "~3.0,>=3.0.5",
    "behat/mink-extension": "~2.0",
    "drupal/drupal-driver": "~1.0@dev",
    "drupal/drupal-extension": "~3.0"
  },
  "require-dev": {
    "phpspec/phpspec": "~2.0",
    "phpunit/phpunit": "3.7.*",
    "behat/mink-zombie-driver": "^1.2"
  },
  "autoload": {
    "psr-0": {
      "Roomify\\RoomifyExtension": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev",
      "dev-d8": "2.0.x-dev"
    }
  }
}